import java.util.Scanner;
import java.util.List;
import java.util.Iterator;
import java.util.ListIterator;
import java.util.ArrayList;
// import java.util.Stream;

class Main {
    public static void main(String[] args) {
        
        List<String> TopFruits = new ArrayList<>();
        System.out.println("Is TopFruits is Empty :"+TopFruits.isEmpty());
        
        TopFruits.add("Banana");
        TopFruits.add("Apple");
        TopFruits.add("Papaya");
        TopFruits.add("Pineaple");
        
        Iterator<String> i = TopFruits.iterator();
        while(i.hasNext()){
           String fruits = (String) i.next();
           System.out.println("TopFruits with Iterator :"+fruits);
       }
    
       ListIterator<String> iterate = TopFruits.listIterator();
       while(iterate.hasNext()){
           String fruits = (String) iterate.next();
           System.out.println("TopFruits next :"+fruits);
       }
       while(iterate.hasPrevious()){
           String fruits = (String) iterate.previous();
           System.out.println("TopFruits previous :"+fruits);
       }
       
       TopFruits.stream().forEach( fruits -> System.out.println("TopFruits :"+fruits)); 
       
       
}
}
